<script src="//cdnjs.cloudflare.com/ajax/libs/jquery-backstretch/2.0.4/jquery.backstretch.min.js" type="text/javascript"></script>
<script>
   if ($( window ).width() > 600 ){
    $("#backstrechImage").css('display','block');
    $("#backstrechImage").css('height','196px');
    $("#backstrechImage").css('width','100%');
    $("#backstrechImage").backstretch('<%= image %>');
   }
</script>
